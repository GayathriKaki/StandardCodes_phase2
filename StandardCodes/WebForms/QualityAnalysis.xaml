                            <Button Style="{StaticResource Buttonstyle}" Width="50"  Height="30" Grid.Column="1" Grid.Row="22" Margin="10,1,0,3" Name="btnSubmit" HorizontalAlignment="Left"  Click="btnBASave_Click">Save</Button>
                            <CheckBox Grid.Column="1" Grid.Row="18" HorizontalAlignment="Left" Margin="11,5,0,4" Name="chkActive"  IsChecked="True" IsEnabled="False"></CheckBox>
                            <ComboBox  ItemsSource="{Binding}" Style="{StaticResource ComboBoxstyleBA}"    SelectedValuePath="PROVINCE_STATE" DisplayMemberPath="MY_COLUMN"  IsSynchronizedWithCurrentItem="True"    Grid.Column="1" Grid.Row="7" HorizontalAlignment="Left" Margin="11,2,0,2" Name="cbostate"></ComboBox>
                            <ComboBox   Grid.Column="1" Grid.Row="15" HorizontalAlignment="Left"  SelectedIndex="0" Margin="11,2,0,0" Name="ddlSource" Style="{StaticResource ComboBoxstyleBA}">
                                <ComboBoxItem>PI</ComboBoxItem>
                            </ComboBox>
