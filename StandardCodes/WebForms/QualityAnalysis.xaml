<Page x:Class="StandardCodes.WebForms.QualityAnalysis"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dg="clr-namespace:Microsoft.Windows.Controls;assembly=WpfToolkit"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:col="clr-namespace:System.Collections;assembly=mscorlib"
    xmlns:cm="clr-namespace:System.ComponentModel;assembly=WindowsBase" 
    xmlns:data="clr-namespace:System.Windows.Data;assembly=PresentationFramework"
    xmlns:local="clr-namespace:StandardCodes"      
    Title="QualityAnalysis" Name="PageQA" Height="400">
    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Themes/ScTheme.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>

    <Grid Name="myGrid" VerticalAlignment="Top" Margin="0" Height="390">       
        
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0"  />
            <ColumnDefinition Width="*" MinWidth="500" />
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
                <RowDefinition Height="18"/>
            <RowDefinition Height="Auto" MinHeight="23" />           
        </Grid.RowDefinitions>

        <!--the Business Associate  DataGrid-->

        <dg:DataGrid x:Name="DataGrid_BA" KeyboardNavigation.AcceptsReturn="True"
                     HorizontalScrollBarVisibility="Visible"  VerticalScrollBarVisibility="Auto"
                      CellEditEnding="DataGrid_BA_CellEditEnding" MinWidth="950" MaxHeight="365"
                      Grid.Row="1" Width="{Binding ElementName=myGrid, Path=ActualWidth}"
                     Grid.Column="1"  RowHeaderWidth="0" 
                    AutoGenerateColumns="False"                                       
                     SelectionUnit="FullRow"  Style="{StaticResource DefaultGridStyle}"                   
                   
                     SelectionMode="Extended"                     
                     CanUserAddRows="False"      
                     RowHeaderStyle="{StaticResource DefaultRowHeaderStyle}" 
                     CellStyle="{StaticResource defaultCellStyle}" 
                     ColumnHeaderStyle="{StaticResource defaultColumnHeaderStyle}"                     
                     RowBackground="White"    
                     AlternatingRowBackground="AliceBlue"
                     AlternationCount="2" 
                     AreRowDetailsFrozen="True" Margin="0,0,0,0" SelectionChanged="DataGrid_BA_SelectionChanged">

            <dg:DataGrid.Columns>
             

                <dg:DataGridTextColumn   Header="ID" Width="40"  Binding="{Binding Path=BA_ID}"  IsReadOnly="True"                                        
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>


                <dg:DataGridTextColumn Header="Standard Code" Width="90"  Binding="{Binding Path=STANDARDCODE}"   >
                    <dg:DataGridTextColumn.EditingElementStyle>
                        <Style TargetType="TextBox">
                            <Setter Property="MaxLength" Value="20"/>
                            <Setter Property="FontWeight" Value="Bold" />
                        </Style>
                    </dg:DataGridTextColumn.EditingElementStyle>
                </dg:DataGridTextColumn>

                <dg:DataGridTextColumn Header="Requester"   Binding="{Binding Path=FULLNAME}" Width="80"  IsReadOnly="True"                                        
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>


                <dg:DataGridComboBoxColumn Header="Status" SelectedItemBinding="{Binding Path=BA_TYPE}" >
                    <dg:DataGridComboBoxColumn.ItemsSource>
                        <col:ArrayList>
                            <sys:String>Pending</sys:String>
                            <sys:String>Research</sys:String>
                            <sys:String>Completed</sys:String>
                            <sys:String>On Hold</sys:String>
                        </col:ArrayList>
                    </dg:DataGridComboBoxColumn.ItemsSource>
                </dg:DataGridComboBoxColumn>

                <dg:DataGridComboBoxColumn Header="Owner"  SelectedValueBinding="{Binding OWNER}" 
                                           TextBinding="{Binding OWNER}"
                                           SelectedItemBinding="{Binding Path=OWNER}"  
                                           ItemsSource="{Binding Source={StaticResource OwnerCollection}}"
                                           SelectedValuePath="OWNER" DisplayMemberPath="OWNER"></dg:DataGridComboBoxColumn>

              
                <dg:DataGridTextColumn   Header="Date Posted" Width="130" Binding="{Binding Path=DATE_ENTERED}"  IsReadOnly="True"                                        
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridTextColumn   Header="Updated Date" Width="130" Binding="{Binding Path=PI_REC_UPD_DATE}"  IsReadOnly="True"                                        
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridTextColumn Header="Priority" Width="45"  Binding="{Binding Path=REQUEST_ORDER}"    IsReadOnly="True"                                       
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>


                <dg:DataGridTextColumn Header="Comments"  Width="100" Binding="{Binding Path=REMARKS}" >
                    <dg:DataGridTextColumn.EditingElementStyle>
                        <Style TargetType="TextBox">
                            <Setter Property="MaxLength" Value="240"/>
                            <Setter Property="FontWeight" Value="Bold" />
                        </Style>
                    </dg:DataGridTextColumn.EditingElementStyle>
                </dg:DataGridTextColumn>

                <dg:DataGridTemplateColumn    Header="Save" Width="50">
                    <dg:DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Content="Save"    Click="SaveBAButton_Click" />
                        </DataTemplate>
                    </dg:DataGridTemplateColumn.CellTemplate>
                </dg:DataGridTemplateColumn>

                <dg:DataGridTemplateColumn Header="View Detail" Width="80">
                    <dg:DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Content="View Detail" Click="ViewBAButton_Click" />
                        </DataTemplate>
                    </dg:DataGridTemplateColumn.CellTemplate>
                </dg:DataGridTemplateColumn>

                <dg:DataGridTextColumn   Header="BA_NAME" Binding="{Binding Path=BA_NAME}" Visibility="Hidden"  Width="0"  MinWidth="0"   IsReadOnly="True"                                         
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridTextColumn   Header="PI_USER_ID" Binding="{Binding Path=PI_USER_ID}" Width="0"  MinWidth="0"  Visibility="Hidden" IsReadOnly="True"                                        
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridComboBoxColumn Header="Status1" Width="0"  MinWidth="0"  SelectedItemBinding="{Binding Path=BA_TYPE}" >
                    <dg:DataGridComboBoxColumn.ItemsSource>
                        <col:ArrayList>
                            <sys:String>Pending</sys:String>
                            <sys:String>Research</sys:String>
                            <sys:String>Completed</sys:String>
                            <sys:String>On Hold</sys:String>
                        </col:ArrayList>
                    </dg:DataGridComboBoxColumn.ItemsSource>

                </dg:DataGridComboBoxColumn>

            </dg:DataGrid.Columns>
        </dg:DataGrid>


        <!--the Business Associate  DataGrid-->

        <!-- Field Res Formation -->

        <local:CustomDataGrid  x:Name="DataGrid_FRF" KeyboardNavigation.AcceptsReturn="True" VerticalAlignment="Top" 
                               VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto"
                               CellEditEnding="DataGridFRF_CellEditEnding" MinWidth="950" MaxHeight="360"
                     Grid.Row="1"   RowHeaderWidth="0"
                     Grid.Column="1" 
                     AutoGenerateColumns="False"                                       
                     SelectionUnit="FullRow"
                    
                   Style="{StaticResource DefaultGridStyle}"  
                     SelectionMode="Extended"                     
                     CanUserAddRows="False"     
                     CellStyle="{StaticResource defaultCellStyle}" 
                               RowHeaderStyle="{StaticResource DefaultRowHeaderStyle}" 
                     ColumnHeaderStyle="{StaticResource defaultColumnHeaderStyle}"                     
                     RowBackground="White" 
                     AlternatingRowBackground="AliceBlue" 
                     AlternationCount="2" 
                     AreRowDetailsFrozen="True" Margin="0,0,0,0" SelectionChanged="DataGrid_FRF_SelectionChanged">
            <local:CustomDataGrid.Columns>
               

                <dg:DataGridTextColumn   Header="ID" Width="40"  Binding="{Binding Path=FLD_KEY}"    IsReadOnly="True"                                      
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

              
                
                <dg:DataGridTextColumn Header="Standard Code" Width="90"  Binding="{Binding Path=FIELD_ID}"   >
                    <dg:DataGridTextColumn.EditingElementStyle>
                        <Style TargetType="TextBox">
                            <Setter Property="MaxLength" Value="12"/>
                            <Setter Property="FontWeight" Value="Bold" />
                        </Style>
                    </dg:DataGridTextColumn.EditingElementStyle>
                </dg:DataGridTextColumn>

                <dg:DataGridTextColumn Header="Requestor" Binding="{Binding Path=FIELD_NAME}" Width="80"  IsReadOnly="True"                                        
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridComboBoxColumn Header="Status" SelectedItemBinding="{Binding Path=STATUS}" >
                    <dg:DataGridComboBoxColumn.ItemsSource>
                        <col:ArrayList>
                            <sys:String>Pending</sys:String>
                            <sys:String>Research</sys:String>
                            <sys:String>Completed</sys:String>
                            <sys:String>On Hold</sys:String>
                        </col:ArrayList>
                    </dg:DataGridComboBoxColumn.ItemsSource>
                </dg:DataGridComboBoxColumn>

                <dg:DataGridComboBoxColumn Header="Owner"  SelectedValueBinding="{Binding OWNER}" 
                                           TextBinding="{Binding OWNER}"
                                           SelectedItemBinding="{Binding Path=OWNER}"  
                                           ItemsSource="{Binding Source={StaticResource OwnerCollection}}"
                                           SelectedValuePath="OWNER" DisplayMemberPath="OWNER"></dg:DataGridComboBoxColumn>

                <dg:DataGridTextColumn   Header="Date Posted" Width="130" Binding="{Binding Path=DATE_ENTERED}"   IsReadOnly="True"                                       
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridTextColumn   Header="Date Updated" Width="130" Binding="{Binding Path=PI_REC_UPD_DATE}"   IsReadOnly="True"                                       
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridTextColumn Header="Priority" Width="45"  Binding="{Binding Path=REQUEST_ORDER}"   IsReadOnly="True"                                       
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridTextColumn Header="Comments" Binding="{Binding Path=REMARK}" >
                    <dg:DataGridTextColumn.EditingElementStyle>
                        <Style TargetType="TextBox">
                            <Setter Property="MaxLength" Value="240"/>
                            <Setter Property="FontWeight" Value="Bold" />
                        </Style>
                    </dg:DataGridTextColumn.EditingElementStyle>
                </dg:DataGridTextColumn>

                <dg:DataGridTemplateColumn Header="Save" Width="50">
                    <dg:DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Content="Save" Click="SaveFRFButton_Click" />
                        </DataTemplate>
                    </dg:DataGridTemplateColumn.CellTemplate>
                </dg:DataGridTemplateColumn>

                <dg:DataGridTemplateColumn Header="View Detail" Width="80">
                    <dg:DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Content="View Detail" Click="ViewFRFButton_Click"  />
                        </DataTemplate>
                    </dg:DataGridTemplateColumn.CellTemplate>
                </dg:DataGridTemplateColumn>

                <dg:DataGridTextColumn   Header="FIELD_NAME" Binding="{Binding Path=FIELD_NAME}" Visibility="Hidden" Width="0"  MinWidth="0"    IsReadOnly="True"                                         
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridTextColumn   Header="PI_USER_ID" Binding="{Binding Path=PI_USER_ID}" Visibility="Hidden" Width="0"  MinWidth="0"  IsReadOnly="True"                                        
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>
                
                <dg:DataGridComboBoxColumn Header="Status1" Width="0"  MinWidth="0"   SelectedItemBinding="{Binding Path=STATUS}" >
                    <dg:DataGridComboBoxColumn.ItemsSource>
                        <col:ArrayList>
                            <sys:String>Pending</sys:String>
                            <sys:String>Research</sys:String>
                            <sys:String>Completed</sys:String>
                            <sys:String>On Hold</sys:String>
                        </col:ArrayList>
                    </dg:DataGridComboBoxColumn.ItemsSource>
                </dg:DataGridComboBoxColumn>

               
            </local:CustomDataGrid.Columns>
        </local:CustomDataGrid>

        <!-- FRF Ends -->


        <!-- MISC Res Formation -->

        <dg:DataGrid x:Name="DataGrid_MISC"  KeyboardNavigation.AcceptsReturn="True" VerticalAlignment="Top"
                     VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto"
                     CellEditEnding="DataGridMISC_CellEditEnding" 
                     Grid.Row="1"   MinWidth="950" MaxHeight="360"
                     Grid.Column="1"   RowHeaderWidth="0"
                     AutoGenerateColumns="False"                                       
                     SelectionUnit="FullRow"                    
                     Style="{StaticResource DefaultGridStyle}"  
                     SelectionMode="Extended"                     
                     CanUserAddRows="False"      
                     CellStyle="{StaticResource defaultCellStyle}" 
                     ColumnHeaderStyle="{StaticResource defaultColumnHeaderStyle}"    
                     RowHeaderStyle="{StaticResource DefaultRowHeaderStyle}" 
                     RowBackground="White"  
                     AlternatingRowBackground="AliceBlue"
                     AlternationCount="2"
                     AreRowDetailsFrozen="True" Margin="0,0,0,0" SelectionChanged="DataGrid_MISC_SelectionChanged" HeadersVisibility="All" GridLinesVisibility="All">

            <dg:DataGrid.Columns>
               
                <dg:DataGridTextColumn     Header="MIS_KEY" Width="65"  Binding="{Binding Path=MISC_KEY}"   IsReadOnly="True"                                       
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridTextColumn   Header="Standard Code" Width="95"  Binding="{Binding Path=BA_ID}"   IsReadOnly="False">
                    <dg:DataGridTextColumn.EditingElementStyle>
                        <Style TargetType="TextBox">
                            <Setter Property="MaxLength" Value="50"/>
                            <Setter Property="FontWeight" Value="Bold" />
                        </Style>
                    </dg:DataGridTextColumn.EditingElementStyle>
                </dg:DataGridTextColumn>

                  
                <dg:DataGridTextColumn Header="User Name" Binding="{Binding Path=USER_NAME}" Width="80"   IsReadOnly="True"                                       
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridComboBoxColumn Header="Status" SelectedItemBinding="{Binding Path=REQUEST_TYPE}" >
                    <dg:DataGridComboBoxColumn.ItemsSource>
                        <col:ArrayList>
                            <sys:String>Pending</sys:String>
                            <sys:String>Research</sys:String>
                            <sys:String>Completed</sys:String>
                            <sys:String>On Hold</sys:String>

                        </col:ArrayList>
                    </dg:DataGridComboBoxColumn.ItemsSource>

                </dg:DataGridComboBoxColumn>

                <dg:DataGridComboBoxColumn Header="Owner"  SelectedValueBinding="{Binding OWNER}" 
                                           TextBinding="{Binding OWNER}"
                                           SelectedItemBinding="{Binding Path=OWNER}"  
                                           ItemsSource="{Binding Source={StaticResource OwnerCollection}}"
                                           SelectedValuePath="OWNER" DisplayMemberPath="OWNER"></dg:DataGridComboBoxColumn>

                <dg:DataGridTextColumn   Header="Date Posted" Width="130" Binding="{Binding Path=DATE_ENTERED}"     IsReadOnly="True"                                     
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridTextColumn   Header="Date Updated"  Width="130" Binding="{Binding Path=PI_REC_UPD_DATE}"     IsReadOnly="True"                                     
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>

                <dg:DataGridTextColumn Header="Priority"  Width="50" Binding="{Binding Path=REQUEST_ORDER}"    IsReadOnly="True"                                       
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>


                <dg:DataGridTextColumn Header="Comments" Binding="{Binding Path=REMARKS}" >
                    <dg:DataGridTextColumn.EditingElementStyle>
                        <Style TargetType="TextBox">
                            <Setter Property="MaxLength" Value="240"/>
                            <Setter Property="FontWeight" Value="Bold" />
                        </Style>
                    </dg:DataGridTextColumn.EditingElementStyle>
                </dg:DataGridTextColumn>




                <dg:DataGridTemplateColumn Header="Save" Width="50">
                    <dg:DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Content="Save" Click="SaveMISCButton_Click" />
                        </DataTemplate>
                    </dg:DataGridTemplateColumn.CellTemplate>
                </dg:DataGridTemplateColumn>

                <dg:DataGridTemplateColumn Header="View Detail" Width="80">
                    <dg:DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button Content="View Detail" Click="ViewMISCButton_Click" />
                        </DataTemplate>
                    </dg:DataGridTemplateColumn.CellTemplate>
                </dg:DataGridTemplateColumn>


                <dg:DataGridTextColumn   Header="USER_NAME" Width="0"  MinWidth="0"  Binding="{Binding Path=PI_USER_ID}" Visibility="Hidden" IsReadOnly="True"                                        
                                       EditingElementStyle="{StaticResource textBoxEditingStyle}"/>
                <dg:DataGridComboBoxColumn Header="Status1" SelectedItemBinding="{Binding Path=REQUEST_TYPE}" Width="0"  MinWidth="0" >
                    <dg:DataGridComboBoxColumn.ItemsSource>
                        <col:ArrayList>
                            <sys:String>Pending</sys:String>
                            <sys:String>Research</sys:String>
                            <sys:String>Completed</sys:String>
                            <sys:String>On Hold</sys:String>

                        </col:ArrayList>
                    </dg:DataGridComboBoxColumn.ItemsSource>

                </dg:DataGridComboBoxColumn>


            </dg:DataGrid.Columns>
        </dg:DataGrid>

        <!-- MISC Ends -->


        <!--column customizing-->
        
        <!-- Busines Associate popup -->
        <Popup  Name="PopupBA" Visibility="Hidden"  IsOpen="False" Width="Auto" Height="Auto"    >
            <ScrollViewer  Name="scrollViewer1" VerticalScrollBarVisibility="Auto">
