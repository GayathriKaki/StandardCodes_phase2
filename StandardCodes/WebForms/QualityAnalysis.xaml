                            <Label  Style="{StaticResource Labelstyle}" Grid.Row="17" Margin="3,0,4,0" Name="label18">Fax</Label>
                            <Label  Style="{StaticResource Labelstyle}" Grid.Row="18" Margin="3,0,4,0" Name="label19">Active</Label>
                            <Label  Style="{StaticResource Labelstyle}" Grid.Row="19" Margin="3,0,4,0" Name="label20">Comments</Label>
